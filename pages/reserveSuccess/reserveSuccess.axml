<view class="root-container">
  <image class="book-image" mode="aspectFill" src="{{bookPicUrl}}" />
  <view class="reserve-success">预订成功</view>
  <view class="counting-container">
    <view>
      <text class="count-time">请在<text class="height-light">{{leftMinute}}</text>分<text class="height-light">{{leftSecond}}</text>秒内</text>
    </view>
     <text class="classname">到{{position}}领取</text>
  </view>
  <view class="finish-type-in" onTap="cancelReserve">取消预定</view>
</view>